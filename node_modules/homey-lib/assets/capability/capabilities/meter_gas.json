  "$flow": {
    "triggers": [
      {
        "id": "meter_gas_changed",
        "title": {
          "en": "The gas meter changed",
          "nl": "De gasmeter is veranderd",
          "de": "Der Gaszähler hat sich geändert",
          "fr": "Le compteur de gaz a changé",
          "it": "Il contatore del gas è cambiato"
        },
        "tokens": [
          {
            "name": "$id",
