{
  "type": "number",
  "title": {
    "en": "Gas Meter",
    "nl": "Gasmeter",
    "de": "Gaszähler",
    "fr": "Compteur de gaz",
    "it": "Contatore del gas"
  },
  "min": 0,
  "decimals": 2,
  "chartType": "spline",
  "units": {
    "en": "m³"
  },
  "desc": {
    "en": "Gas usage in Cubic Meter (m³)",
    "nl": "Gas gebruik in Kubieke Meter (m³)",
    "de": "Gasverbrauch in Kubikmeter (m³)",
