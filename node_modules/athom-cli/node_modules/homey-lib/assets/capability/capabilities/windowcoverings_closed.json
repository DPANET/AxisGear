{
  "type": "boolean",
  "title": {
    "en": "Closed",
    "nl": "Gesloten"
  },
  "getable": true,
  "setable": true,
  "uiComponent": "toggle",
  "$flow": {
    "triggers": [
      {
        "id": "windowcoverings_closed_true",
        "title": {
          "en": "Closed",
          "nl": "Gesloten"
        }
      },
      {
        "id": "windowcoverings_closed_false",
        "title": {
          "en": "Opened",
          "nl": "Geopend"
        }
      }
    ],
    "conditions": [
      {
        "id": "closed",
        "title": {
